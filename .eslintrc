{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
   "env": {
     "es6": true,
     "node": true,
  },
  "rules": {
    "no-var": "error",
    "prefer-const": "error",
    "indent": ["error", 2]
  }
}
